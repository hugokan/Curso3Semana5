<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.quinteropro.petagram">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Prueba">

        </activity>
        <activity
            android:name=".menu.Acercade"
            android:parentActivityName=".ListaMascotas">

            <!-- Soporte para version 4.0 o menores -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MascotasFavoritas5" />
        </activity>
        <activity
            android:name=".menu.Contacto"
            android:parentActivityName=".ListaMascotas">

            <!-- Soporte para version 4.0 o menores -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MascotasFavoritas5" />
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ListaMascotas" />
        <activity
            android:name=".MascotasFavoritas5"
            android:parentActivityName=".ListaMascotas">

            <!-- Soporte para version 4.0 o menores -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MascotasFavoritas5" />
        </activity>
    </application>

</manifest>